<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Lib_SlidingMenu">
        <attr name="mode">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="viewAbove" format="reference" />
        <attr name="viewBehind" format="reference" />
        <attr name="behindOffset" format="dimension" />
        <attr name="behindWidth" format="dimension" />
        <attr name="behindScrollScale" format="float" />
        <attr name="touchModeAbove">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
            <enum name="none" value="2" />
        </attr>
        <attr name="touchModeBehind">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
            <enum name="none" value="2" />
        </attr>
        <attr name="shadowDrawable" format="reference" />
        <attr name="shadowWidth" format="dimension" />
        <attr name="fadeEnabled" format="boolean" />
        <attr name="fadeDegree" format="float" />
        <attr name="selectorEnabled" format="boolean" />
        <attr name="selectorDrawable" format="reference" />
    </declare-styleable>
    <declare-styleable name="Lib_DragSortListView">
        <attr name="collapsed_height" format="dimension" />
        <attr name="drag_scroll_start" format="float" />
        <attr name="max_drag_scroll_speed" format="float" />
        <attr name="float_background_color" format="color" />
        <attr name="remove_mode">
            <enum name="clickRemove" value="0" />
            <enum name="flingRemove" value="1" />
        </attr>
        <attr name="track_drag_sort" format="boolean" />
        <attr name="float_alpha" format="float" />
        <attr name="slide_shuffle_speed" format="float" />
        <attr name="remove_animation_duration" format="integer" />
        <attr name="drop_animation_duration" format="integer" />
        <attr name="drag_enabled" format="boolean" />
        <attr name="sort_enabled" format="boolean" />
        <attr name="remove_enabled" format="boolean" />
        <attr name="drag_start_mode">
            <enum name="onDown" value="0" />
            <enum name="onMove" value="1" />
            <enum name="onLongPress" value="2" />
        </attr>
        <attr name="drag_handle_id" format="integer" />
        <attr name="fling_handle_id" format="integer" />
        <attr name="click_remove_id" format="integer" />
        <attr name="use_default_controller" format="boolean" />
    </declare-styleable>
    <!-- ViewPager -->
    <declare-styleable name="Lib_ViewPager">
        <attr name="scrollable" format="boolean" />
        <attr name="autoScroll" format="boolean" />
        <attr name="interval" format="integer" />
        <attr name="viewPagerHelp">
            <enum name="scrollable" value="0" />
            <enum name="autoScroll" value="1" />
            <enum name="interval" value="2" />
        </attr>
    </declare-styleable>
    <!--Background-->
    <declare-styleable name="Lib_ShapeBackground">
        <attr name="bottomLeftRadius" format="dimension" />
        <attr name="bottomRightRadius" format="dimension" />
        <attr name="radius" format="dimension" />
        <attr name="topLeftRadius" format="dimension" />
        <attr name="topRightRadius" format="dimension" />
        <attr name="solidColor" format="color" />
        <attr name="solidColor2" format="color" />
        <attr name="strokeColor" format="color" />
        <attr name="strokeColor2" format="color" />
        <attr name="strokeDashWidth" format="dimension" />
        <attr name="strokeDashGap" format="dimension" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="gradientStartColor" format="color" />
        <attr name="gradientStartColor2" format="color" />
        <attr name="gradientCenterColor" format="color" />
        <attr name="gradientCenterColor2" format="color" />
        <attr name="gradientEndColor" format="color" />
        <attr name="gradientEndColor2" format="color" />
        <attr name="status">
            <enum name="pressed" value="0" />
            <enum name="enabled" value="1" />
            <enum name="checked" value="2" />
            <enum name="selected" value="3" />
        </attr>
        <attr name="help">
            <enum name="bottomLeftRadius" value="0" />
            <enum name="bottomRightRadius" value="1" />
            <enum name="topLeftRadius" value="2" />
            <enum name="topRightRadius" value="3" />
            <enum name="radius" value="4" />
            <enum name="solidColor" value="5" />
            <enum name="solidColor2" value="6" />
            <enum name="strokeColor" value="7" />
            <enum name="strokeColor2" value="8" />
            <enum name="strokeDashWidth" value="9" />
            <enum name="strokeDashGap" value="10" />
            <enum name="strokeWidth" value="11" />
            <enum name="gradientStartColor" value="12" />
            <enum name="gradientStartColor2" value="13" />
            <enum name="gradientCenterColor" value="14" />
            <enum name="gradientCenterColor2" value="15" />
            <enum name="gradientEndColor" value="16" />
            <enum name="gradientEndColor2" value="17" />
            <enum name="status" value="18" />
        </attr>
    </declare-styleable>
    <!--TextView-->
    <declare-styleable name="Lib_TextViewColor">
        <attr name="textColorStatus">
            <enum name="pressed" value="0" />
            <enum name="enabled" value="1" />
            <enum name="checked" value="2" />
            <enum name="selected" value="3" />
        </attr>
        <attr name="textColor2" format="color" />
    </declare-styleable>
    <declare-styleable name="Lib_AbsListView">
        <attr name="isExpand" format="boolean" />
    </declare-styleable>
</resources>