<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Lib_SlidingMenu">
        <attr name="lib_mode">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="lib_viewAbove" format="reference" />
        <attr name="lib_viewBehind" format="reference" />
        <attr name="lib_behindOffset" format="dimension" />
        <attr name="lib_behindWidth" format="dimension" />
        <attr name="lib_behindScrollScale" format="float" />
        <attr name="lib_touchModeAbove">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
            <enum name="none" value="2" />
        </attr>
        <attr name="lib_touchModeBehind">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
            <enum name="none" value="2" />
        </attr>
        <attr name="lib_shadowDrawable" format="reference" />
        <attr name="lib_shadowWidth" format="dimension" />
        <attr name="lib_fadeEnabled" format="boolean" />
        <attr name="lib_fadeDegree" format="float" />
        <attr name="lib_selectorEnabled" format="boolean" />
        <attr name="lib_selectorDrawable" format="reference" />
    </declare-styleable>
    <declare-styleable name="Lib_DragSortListView">
        <attr name="collapsed_height" format="dimension" />
        <attr name="drag_scroll_start" format="float" />
        <attr name="max_drag_scroll_speed" format="float" />
        <attr name="float_background_color" format="color" />
        <attr name="remove_mode">
            <enum name="clickRemove" value="0" />
            <enum name="flingRemove" value="1" />
        </attr>
        <attr name="track_drag_sort" format="boolean" />
        <attr name="float_alpha" format="float" />
        <attr name="slide_shuffle_speed" format="float" />
        <attr name="remove_animation_duration" format="integer" />
        <attr name="drop_animation_duration" format="integer" />
        <attr name="drag_enabled" format="boolean" />
        <attr name="sort_enabled" format="boolean" />
        <attr name="remove_enabled" format="boolean" />
        <attr name="drag_start_mode">
            <enum name="onDown" value="0" />
            <enum name="onMove" value="1" />
            <enum name="onLongPress" value="2" />
        </attr>
        <attr name="drag_handle_id" format="integer" />
        <attr name="fling_handle_id" format="integer" />
        <attr name="click_remove_id" format="integer" />
        <attr name="use_default_controller" format="boolean" />
    </declare-styleable>
    <!-- ViewPager -->
    <declare-styleable name="Lib_ViewPager">
        <attr name="lib_scrollable" format="boolean" />
        <attr name="lib_autoScroll" format="boolean" />
        <attr name="lib_interval" format="integer" />
    </declare-styleable>
    <!--Background-->
